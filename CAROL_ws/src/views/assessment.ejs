<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Molding Assessment</title>
  <style>
    body, html { margin: 0; padding: 0; height: 100%; font-family: sans-serif; }
    #formbricks-container { width: 100%; height: 100%; }
  </style>
</head>
<body>
  <div id="formbricks-container"></div>
  <script>
    (function () {
      var formbricks = window.formbricks || function () { (formbricks.q = formbricks.q || []).push(arguments) };
      window.formbricks = formbricks;
      var script = document.createElement("script");
      script.async = true;
      script.src = "<%= formbricksUrl %>/api/packages/website";
      document.head.appendChild(script);
    })();
    formbricks("init", {
      environmentId: "<%= environmentId %>",
      apiHost: "<%= formbricksUrl %>",
      userId: "<%= employeeId %>",
    });
    formbricks("display", "<%= surveyId %>", { containerId: "formbricks-container" });
  </script>
</body>
</html>
